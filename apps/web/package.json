    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "latest",
    "@sveltejs/kit": "latest",
    "eslint-plugin-svelte": "^2.27.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "eslint": "^8.39.0",
    "@typescript-eslint/parser": "^5.59.1",
    "vite": "^4.3.2"
  },
  "type": "module"
}
