{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "APP_ENV=development NODE_ENV=development nodemon --watch src -e ts,tsx --exec tsx src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "lint": "eslint --ext .ts src --fix",
    "test:unit": "ava --verbose --serial --timeout=10m --fail-fast --concurrency=1 --watch"
  },
  "dependencies": {
    "@koa/cors": "4.0.0",
    "@koa/router": "12.0.0",
    "@prisma/client": "^4.13.0",
    "fp-ts": "^2.14.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "2.14.2",
    "koa-bodyparser": "4.4.0",
    "koa-helmet": "7.0.2",
    "koa-logger": "3.2.1",
    "koa-mount": "4.0.0",
    "module-alias": "2.2.2",
    "reflect-metadata": "0.1.13",
    "tsx": "^3.12.7",
    "winston": "3.8.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "@types/koa": "2.13.6",
    "@types/koa-bodyparser": "4.3.10",
    "@types/koa-logger": "^3.1.2",
    "@types/koa__cors": "4.0.0",
    "@types/koa__router": "^12.0.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "18.16.3",
    "ava": "^5.2.0",
    "eslint-config-custom": "workspace:eslint-config-custom",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "prisma": "4.14.0",
    "tsconfig": "workspace:tsconfig",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "mongodb": "^5.1.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "nodeArguments": [
      "--loader=tsx"
    ]
  }
}
